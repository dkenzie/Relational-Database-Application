<Window x:Class="CPSC304_Project.SelectUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CPSC304_Project"
        mc:Ignorable="d" WindowStyle="None" ResizeMode="NoResize"
        Title="SelectUserWindow" Height="200" Width="300">
    <Canvas>
        <Label Content="Select User" FontSize="16"/>
        <Separator Height="73" Margin="0" Canvas.Top="-11" Width="290"/>
        <Button Name="CloseButton" Content="X" Canvas.Left="270" Canvas.Top="8" Width="20" Height="19" Click="CloseButton_Click"/>
        <ScrollViewer Height="168" Width="300" Canvas.Top="32">
            <StackPanel Name="MainStackPanel">
                
            </StackPanel>
        </ScrollViewer>

    </Canvas>
</Window>
